{
	"name": "dataflowCTDL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "datasetsql",
						"type": "DatasetReference"
					},
					"name": "storage"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataset_csv",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {first name} as string,",
				"          {last name} as string,",
				"          {E.mail} as string,",
				"          phone as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> storage",
				"storage filter(!isNull(UserName)) ~> filter1",
				"filter1 derive(UserName = 'test') ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          UserName as string,",
				"          Gender as string,",
				"          Phone as string,",
				"          Email as string,",
				"          CreditCard as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}